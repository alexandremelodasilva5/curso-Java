package application;

import java.util.ArrayList;
import java.util.List;

public class Program {

	public static void main(String[] args) {
		List<String> list = new ArrayList<>();
		
		list.add("Alexandre");
		list.add("Maira");
		list.add("Helena");
		
		list.add(1, "Laura");
		
		for (String obj : list) {
			System.out.println(obj);
		}
	
	}

}
